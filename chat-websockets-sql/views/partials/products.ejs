<table class="table">
  <thead>
    <tr>
      <th scope="col">Image</th>
      <th scope="col">Title</th>
      <th scope="col">Price</th>
    </tr>
  </thead>
  <tbody id="table_body">
      <% if (products.length) { %>
        <% products.forEach(function(product) { %>
          <tr>
            <td><img src="<%= product.image %>" alt="" srcset="" width="30px"></td>
            <td><%= product.name %></td>
            <td><%= product.price %></td>
          </tr>
        <% }); %>
      <% } %> 
    </tbody>
  </table>